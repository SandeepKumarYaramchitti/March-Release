platform :ios, '9.0'

target 'Lets Wander' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Lets Wander
   pod 'Firebase/Auth'
   pod 'Firebase/Database'
   pod 'Firebase/Storage'

  target 'Lets WanderTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Lets WanderUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
